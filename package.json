{
  "name": "groot",
  "version": "4.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint groot --format json --force > tslint.out.json",
    "sonar": "npm run lint && sonar-scanner",
    "e2e": "ng e2e",
    "build_lib": "ng build groot --configuration production",
    "npm_pack": "cd dist/groot && npm pack",
    "package": "npm run build_lib && npm run npm_pack",
    "publish": "node scripts/publish.js",
    "release": "npm run build_lib && release-it --disable-metrics && npm run package && npm run publish",
    "build-demo-app": "ng build groot-tester --configuration production"
  },
  "license": "Apache-2.0",
  "private": false,
  "dependencies": {
    "@angular/animations": "^17.3.10",
    "@angular/cdk": "^16.2.12",
    "@angular/common": "^17.3.10",
    "@angular/compiler": "^17.3.10",
    "@angular/core": "^17.3.10",
    "@angular/forms": "^17.3.10",
    "@angular/platform-browser": "^17.3.10",
    "@angular/platform-browser-dynamic": "^17.3.10",
    "@angular/router": "^17.3.10",
    "@fortawesome/fontawesome-free": "^6.4.2",
    "@ng-select/ng-select": "^11.0.0",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "bootstrap": "^5.3.1",
    "core-js": "^3.31.1",
    "file-saver": "^2.0.5",
    "filesize": "^10.0.7",
    "ngx-bootstrap": "^11.0.2",
    "ngx-currency": "^3.0.0",
    "rxjs": "~7.8.1",
    "tslib": "^2.6.1",
    "zone.js": "~0.14.6"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.8",
    "@angular/cli": "^17.3.8",
    "@angular/compiler-cli": "^17.3.10",
    "@release-it/bumper": "^5.1.0",
    "@types/jasmine": "~3.10.2",
    "@types/jasminewd2": "~2.0.10",
    "@types/node": "^12.20.55",
    "codelyzer": "^6.0.2",
    "jasmine-core": "~3.10.1",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.4.2",
    "karma-chrome-launcher": "~3.1.1",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.2",
    "karma-jasmine-html-reporter": "^1.7.0",
    "ng-packagr": "^17.3.0",
    "protractor": "^7.0.0",
    "release-it": "^16.3.0",
    "sonar-scanner": "^3.1.0",
    "ts-node": "~10.9.1",
    "tslint": "~6.1.3",
    "typescript": "~5.4.5"
  }
}