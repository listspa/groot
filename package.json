{
  "name": "groot",
  "version": "0.22.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint groot --format json --force > tslint.out.json",
    "sonar": "npm run lint && sonar-scanner",
    "e2e": "ng e2e",
    "build_lib": "ng build groot",
    "copy-assets": "node scripts/assets-copy.js",
    "npm_pack": "cd dist/groot && npm pack",
    "package": "npm run build_lib && npm run copy-assets && npm run npm_pack",
    "release": "npm run build_lib && release-it --disable-metrics && npm run package && node scripts/publish.js",
    "build-demo-app": "ng build groot-tester --prod",
    "bundle-report": "npm run build-demo-app && webpack-bundle-analyzer dist/groot-tester/stats.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.1.11",
    "@angular/cdk": "^9.2.4",
    "@angular/common": "^9.1.11",
    "@angular/compiler": "^9.1.11",
    "@angular/core": "^9.1.11",
    "@angular/forms": "^9.1.11",
    "@angular/platform-browser": "^9.1.11",
    "@angular/platform-browser-dynamic": "^9.1.11",
    "@angular/router": "^9.1.11",
    "@ng-select/ng-select": "^4.0.1",
    "@ngx-translate/core": "^12.1.2",
    "@ngx-translate/http-loader": "^5.0.0",
    "bootstrap": "^4.5.0",
    "core-js": "^2.6.11",
    "file-saver": "^2.0.2",
    "filesize": "^4.2.1",
    "font-awesome": "^4.7.0",
    "ngx-bootstrap": "^3.3.0",
    "rxjs": "~6.5.5",
    "wrench": "^1.5.9",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.901.9",
    "@angular-devkit/build-ng-packagr": "^0.901.9",
    "@angular/cli": "^9.1.9",
    "@angular/compiler-cli": "^9.1.11",
    "@angular/language-service": "^9.1.11",
    "@types/file-saver": "^2.0.1",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "~8.9.4",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.1.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^9.1.5",
    "protractor": "^5.4.4",
    "release-it": "^10.4.5",
    "scp2": "^0.5.0",
    "sonar-scanner": "^3.1.0",
    "ts-node": "~7.0.0",
    "tsickle": "^0.38.1",
    "tslib": "^1.13.0",
    "tslint": "~5.11.0",
    "typescript": "~3.8.3",
    "webpack-bundle-analyzer": "^3.8.0"
  }
}
