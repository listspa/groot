{
  "name": "groot",
  "version": "3.4.3",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint groot --format json --force > tslint.out.json",
    "sonar": "npm run lint && sonar-scanner",
    "e2e": "ng e2e",
    "build_lib": "ng build groot --configuration production",
    "npm_pack": "cd dist/groot && npm pack",
    "package": "npm run build_lib && npm run npm_pack",
    "deploy": "node scripts/publish.js",
    "release": "npm run build_lib && release-it --disable-metrics && npm run package && npm run deploy",
    "build-demo-app": "ng build groot-tester --configuration production"
  },
  "license": "Apache-2.0",
  "private": false,
  "dependencies": {
    "@angular/animations": "~12.2.17",
    "@angular/cdk": "^11.1.0",
    "@angular/common": "~12.2.17",
    "@angular/compiler": "~12.2.17",
    "@angular/core": "~12.2.17",
    "@angular/forms": "~12.2.17",
    "@angular/platform-browser": "~12.2.17",
    "@angular/platform-browser-dynamic": "~12.2.17",
    "@angular/router": "~12.2.17",
    "@ng-select/ng-select": "^6.1.0",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "bootstrap": "^4.5.3",
    "core-js": "^3.8.3",
    "file-saver": "^2.0.5",
    "filesize": "^6.1.0",
    "font-awesome": "^4.7.0",
    "ngx-bootstrap": "^6.2.0",
    "ngx-currency": "^2.5.2",
    "rxjs": "~6.6.0",
    "tslib": "^2.6.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.18",
    "@angular/cli": "~12.2.18",
    "@angular/compiler-cli": "~12.2.17",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.4.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^12.2.7",
    "protractor": "~7.0.0",
    "release-it": "^10.4.5",
    "sonar-scanner": "^3.1.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.3.5"
  }
}