{
  "name": "groot",
  "version": "0.13.0-alpha.18",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build_lib": "ng build groot",
    "copy-assets": "node scripts/assets-copy.js",
    "npm_pack": "cd dist/groot && npm pack",
    "package": "npm run build_lib && npm run copy-assets && npm run npm_pack",
    "release": "npm run build_lib && release-it --disable-metrics && npm run package && node scripts/publish.js",
    "build-demo-app": "ng build groot-tester --prod",
    "deploy-demo-doc": "npm run build-demo-app && node scripts/release-demo-prj.js",
    "bundle-report": "npm run build-demo-app && webpack-bundle-analyzer dist/groot-tester/stats.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.10",
    "@angular/cdk": "^7.2.10",
    "@angular/common": "^7.2.10",
    "@angular/compiler": "^7.2.10",
    "@angular/core": "^7.2.10",
    "@angular/forms": "^7.2.10",
    "@angular/platform-browser": "^7.2.10",
    "@angular/platform-browser-dynamic": "^7.2.10",
    "@angular/router": "^7.2.10",
    "@ng-select/ng-select": "^2.16.2",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "bootstrap": "^4.3.1",
    "core-js": "^2.6.5",
    "filesize": "^4.1.2",
    "font-awesome": "^4.7.0",
    "ngx-bootstrap": "^3.2.0",
    "rxjs": "~6.3.3",
    "tslib": "^1.9.0",
    "wrench": "^1.5.9",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.12.4",
    "@angular-devkit/build-ng-packagr": "^0.12.4",
    "@angular/cli": "^7.2.4",
    "@angular/compiler-cli": "^7.2.10",
    "@angular/language-service": "^7.2.10",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.5",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^4.7.1",
    "protractor": "~5.4.0",
    "release-it": "^10.3.1",
    "scp2": "^0.5.0",
    "ts-node": "~7.0.0",
    "tsickle": "^0.34.3",
    "tslib": "^1.9.0",
    "tslint": "~5.11.0",
    "typescript": "~3.2.2",
    "webpack-bundle-analyzer": "^3.1.0"
  }
}
